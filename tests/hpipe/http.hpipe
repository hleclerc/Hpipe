read[ name, end ] = any beg_str[ name ] any* end end_str[ name ]

get_line = 
    # ( 's: ' read[ 's', eol ] ) |
    ( any* 'P' { os << "1"; } )

get =
    'GET ' read[ 'url', ' ' ] get_line
    { os << url << " "; }

main = get

beg_training
